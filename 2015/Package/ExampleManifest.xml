<?xml version="1.0" encoding="utf-8" ?>
<!--
    An example package version manifest. Uses the npm's version history
    as it uses Semantic Versioning. Thus, portions of this document are
    copyright the npm developers. Anything else is released into the
    public domain.
-->
<pkg:package name="npm"
             website="http://npmjs.com"
             xmlns:pkg="http://zsnap.org/Schemata/2015/Package">
    <pkg:release version="2.12.0" released="2015-06-19"
                 download="https://github.com/npm/npm/releases/tag/v2.12.0">
        <pkg:modifier type="Obsolete"/>
        <pkg:description>
            This is an example description because I couldn't find a
            suitable release description.
        </pkg:description>
        <pkg:changes>
            <pkg:change type="Addition">
                #8557 normalize-package-data@2.2.1: Allow UNLICENSED and SEE
                LICENSE IN filename in "license" field of package.json.
            </pkg:change>
            <pkg:change type="Fix">
                #8548 Remove extraneous newline from npm view output,
                making it easier to use in shell scripts.
            </pkg:change>
        </pkg:changes>
    </pkg:release>
    <pkg:release version="2.12.1" released="2015-06-26"
                 download="https://github.com/npm/npm/releases/tag/v2.12.1"
                 notes="https://github.com/npm/npm/releases/tag/v2.12.1">
        <!-- Empty description for test purposes. -->
        <pkg:description />
        <pkg:changes>
            <pkg:change type="Removal">This is a test removal.</pkg:change>
        </pkg:changes>
    </pkg:release>
</pkg:package>
